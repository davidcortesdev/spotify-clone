{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@components": [
        "src/components/*"
      ],
      "@icons": [
        "src/icons/*"
      ],
      "@lib": [
        "src/lib/*"
      ],
      "@hooks": [
        "src/hooks/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}